<script type="text/javascript">

document.write("<h1>Skip Multples Function</h1>");

for skipMultiples (var x=1; x <= 50; x++){
    if( x % 7 ) {
        document.write(x);
    } else {
        if( x % 3 == 0 ) {
            document.write("Inspirational quote");
        }
    }
    document.write('<br>'); //line breaks to enhance output readability
}
